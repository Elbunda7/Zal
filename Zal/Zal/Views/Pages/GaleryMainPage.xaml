<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Zal.Views.Pages.GaleryMainPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="galleryEntry"/>
            <Button Text="Vytvořit galerii" Clicked="Button_Clicked"></Button>
            <Image x:Name="mainImage" Source="Splash.png" />
            <Grid x:Name="ContentGrid"         
                  ColumnSpacing="1"
                  RowSpacing="1"
                  HorizontalOptions="FillAndExpand"
                  VerticalOptions="FillAndExpand"
                  >
            </Grid>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>